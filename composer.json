{
    "name": "propel/propel",
    "type": "library",
    "description": "Propel2 is an open-source Object-Relational Mapping (ORM) for PHP 7.0 and up.",
    "keywords": [
        "ORM",
        "persistence",
        "Active Record"
    ],
    "homepage": "http://www.propelorm.org/",
    "license": "MIT",
    "authors": [
        {
            "name": "William Durand",
            "email": "william.durand1@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/yaml": "~2.3||~3.0||~4.0",
        "symfony/console": "~2.3||~3.0||~4.0",
        "symfony/finder": "~2.3||~3.0||~4.0",
        "symfony/validator": "~2.3||~3.0.0||~3.1.0||^3.2.4||~4.0",
        "symfony/filesystem": "~2.3||~3.0||~4.0",
        "symfony/config": "~2.3||~3.0||~4.0",
        "symfony/translation": "~2.3||~3.0||~4.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "monolog/monolog": "~1.3",
        "phpunit/phpunit": "*",
        "rector/rector": "^0.7.26",
        "sllh/composer-versions-check": "^2.0"
    },
    "suggest": {
        "monolog/monolog": "The recommended logging library to use with Propel."
    },
    "autoload": {
        "psr-0": {
            "Propel": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Propel\\Tests\\Bookstore\\": "tests/Fixtures/bookstore/build/classes/Propel/Tests/Bookstore",
            "Propel\\Tests\\": "tests/Propel/Tests"
        }
    },
    "bin": [
        "bin/propel"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
